        "type": "dock",
        "dropshadow": false,
        "content": [
          {
            "type": "dock",
            "dock": "left",
            "v_alignment": "stretch",
            "margin": "0,0,3,0",
            "content": [
              {
                "type": "group",
                "header": "Items",
                "dock": "top",
                "header_content": {
                  "type": "button_popup",
                  "style": "settings",
                  "popup_background": "#FF212121",
                  "layout": "options_popup"
                },
                "content": {
                  "type": "layout",
                  "key": "shared_item_grid"
                }
              },
              {
                "type": "group",
                "header": "Pinned Locations",
                "dock": "left",
                "content": {
                  "type": "recentpins",
                  "style": "wrap",
                  "h_alignment": "stretch",
                  "v_alignment": "stretch",
                  "compact": true
                }
              }
            ]
          },
          {
            "type": "container",
            "background": "#000000",
            "content": {
              "type": "map",
              "maps": [
